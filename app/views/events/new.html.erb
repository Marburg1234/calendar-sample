<h1>イベントの投稿をする</h1>

<div>
  <%= form_with model: @event do |f| %>
    <div>
      <%= f.label :title %>
      <%= f.text_field :title, autofocus: true %>
    </div>
    <div>
      <%= f.label "開始日" %>
      <%= f.text_field :start, id: "start_datepicker", placeholder: "カレンダーを表示" %>
    </div>
    <div>
      <%= f.label "終了日" %>
      <%= f.text_field :end, id: "end_datepicker", placeholder: "カレンダーを表示" %>
    </div>
    <div>
      <%= f.submit "保存する" %>
    </div>
  <% end %>
</div>